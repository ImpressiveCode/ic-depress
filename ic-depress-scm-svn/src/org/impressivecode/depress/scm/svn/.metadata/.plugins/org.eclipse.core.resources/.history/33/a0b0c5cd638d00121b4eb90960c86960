package org.knime.mynode;

import org.knime.core.node.NodeDialogPane;
import org.knime.core.node.NodeFactory;
import org.knime.core.node.NodeView;

/**
 * <code>NodeFactory</code> for the "MyNode01" Node.
 * KNIME
 *
 * @author KNIME
 */
public class MyNode01NodeFactory 
        extends NodeFactory<MyNode01NodeModel> {

    /**
     * {@inheritDoc}
     */
    @Override
    public MyNode01NodeModel createNodeModel() {
        return new MyNode01NodeModel();
    }

    /**
     * {@inheritDoc}
     */
    @Override
    public int getNrNodeViews() {
        return 1;
    }

    /**
     * {@inheritDoc}
     */
    @Override
    public NodeView<MyNode01NodeModel> createNodeView(final int viewIndex,
            final MyNode01NodeModel nodeModel) {
        return new MyNode01NodeView(nodeModel);
    }

    /**
     * {@inheritDoc}
     */
    @Override
    public boolean hasDialog() {
        return true;
    }

    /**
     * {@inheritDoc}
     */
    @Override
    public NodeDialogPane createNodeDialogPane() {
        return new MyNode01NodeDialog();
    }

}

