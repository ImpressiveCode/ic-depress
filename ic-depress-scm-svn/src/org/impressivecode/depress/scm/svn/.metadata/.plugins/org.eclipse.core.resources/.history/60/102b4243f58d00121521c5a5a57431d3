package org.knime.svnplugin.test.functional;

import javax.management.RuntimeErrorException;

import org.junit.After;
import org.junit.Before;
import org.junit.Test;
import org.knime.svnplugin.SVNPluginNodeLogic;

public class SVNPluginNodeLogicTest {

	SVNPluginNodeLogic logic = null;

	@Before
	public void setUp() throws Exception {
		logic = new SVNPluginNodeLogic();
	}

	@After
	public void tearDown() throws Exception {
		logic = null;
	}

	@Test(expected = RuntimeErrorException.class)
	public void testInit() {
		logic.Init();
	}

}
